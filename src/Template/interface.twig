{% if imports is defined and imports|length > 0 %}
{% for import in imports %}
import { {{ import }} } from './{{ import }}{% if addTsExtensionToImports %}.ts{% endif %}';
{% endfor %}

{% endif %}
{% if docComment %}
/**
 * {{ docComment|raw }}
 */
{% endif %}
export interface {{ className }} {
{% for property in properties %}
{% if property.docComment %}
  /**
   * {{ property.docComment|raw }}
   */
{% endif %}
  {{ property.name }}: {{ property|tsType }};
{% endfor %}
}
